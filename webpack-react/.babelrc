{
    "presets": ["@babel/react", "@babel/env"], 
    "plugins": [
      ["@babel/plugin-proposal-decorators", { "legacy": true }],
      ["@babel/plugin-transform-runtime", {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }],
      ["@babel/plugin-proposal-class-properties", { "loose": true }],
      "@babel/plugin-syntax-export-default-from"
    ]
}
  